def pos(n):
    ## Write the code
    for i in range(n-1,-1,-1):
        print(i,end=" ")
def neg(n):
    ##Write the code
        for i in range(n,1):
            print(i,end=" ")
def number_flow(n):
    if n == 0:
        print("already Zero")
    elif n > 0:
        pos(n)
    else:
        neg(n)    
