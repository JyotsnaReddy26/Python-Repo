class Solution:
    def uniqueOccurrences(self, arr: List[int]) -> bool:
        d={}
        for i in range(0,len(arr)):
            if arr[i] in d:
                d[arr[i]]+=1
            else:
                d[arr[i]]=1
        values=list(d.values())
        if len(values)==len(set(values)):
            return True
        else:
            return False
